.page-header
  %h1 Sign up

= form_for(resource, :as => resource_name, :url => registration_path(resource_name), html: {class: "form-horizontal"}) do |form|
  = render form
  .control-group
    .controls
      = form.submit "Sign up", class: "btn"
  .control-group
    .controls
      = render :partial => "devise/shared/links"
